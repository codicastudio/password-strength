{
    "name": "codica-studio/passwordstrength",
    "description": "A random Codica Studio package.",

    "homepage": "https://github.com/codica-studio/passwordstrength",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "mbrown@codicastudio.com",
            "homepage": "https://codicastudio.com",
            "role": "Developer"
        }
    ],

    "require": {
        "php": "^7.4 || ^8.0",
        "illuminate/support": "^8.0",
        "illuminate/translation": "^8.*"
    },
    "require-dev": {
        "illuminate/validation": "^8.*",
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Schuppo\\PasswordStrength\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Schuppo\\PasswordStrength\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Schuppo\\PasswordStrength\\PasswordStrengthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "license": "MIT",
    "config": {
        "discard-changes": true
    }
}
